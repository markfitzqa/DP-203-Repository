											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "ProductName",
											"type": "String",
											"physicalType": "nvarchar"
										},
										"sink": {
											"name": "Product",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "SubcategoryName",
