											"physicalType": "nvarchar"
										},
										"sink": {
											"name": "Subcategory",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "CategoryName",
											"type": "String",
											"physicalType": "nvarchar"
										},
										"sink": {
											"name": "Category",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "Colour",
											"type": "String",
											"physicalType": "nvarchar"
										},
										"sink": {
											"name": "Color",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "StyleSizeProductLine",
											"type": "String",
											"physicalType": "varchar"
										},
										"sink": {
											"name": "StyleSizeProductLine",
											"type": "String",
											"physicalType": "String"
										}
